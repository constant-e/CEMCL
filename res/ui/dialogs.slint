import { StandardButton } from "std-widgets.slint";

export component WarnDialog inherits Dialog {
    title: @tr("Warn");

    in-out property <string> msg;

    Text { text: msg; }
    StandardButton { kind: ok; }
}

export component ErrorDialog inherits Dialog {
    title: @tr("Error");

    in-out property <string> msg;

    Text { text: msg; }
    StandardButton { kind: ok; }
}
